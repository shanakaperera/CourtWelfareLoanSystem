	<queryString>
		<![CDATA[SELECT
    rp.pay_date,
    rp.receipt_code,
    rp.amount,
    rp.payment_type,
    m.member_id,
    m.emp_id,
    m.full_name,
    m.job_title,
    rp.id AS receipt_id,
    rp.receipt_code,
    REPLACE(REPLACE(rp.pay_ids,'[',''),']','') AS pay_ids
FROM
    court_loan.receipt_pay rp
INNER JOIN
    court_loan.member m
ON
    (
        rp.member_id = m.id) 
WHERE  rp.receipt_code=$P{rcpt_code} AND rp.payment_type=$P{pay_type}

ORDER BY rp.id DESC LIMIT 1;]]>
	</queryString>
