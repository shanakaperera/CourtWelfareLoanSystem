	<queryString>
		<![CDATA[SELECT
    m.member_id,
    m.emp_id,
    m.full_name,
    m.address,
    m.job_title,
    sbp.hoi_fee,
    sbp.aci_fee,
    sbp.savings_fee,
    sbp.optional,
    sbp.payment_date
FROM
    court_loan.subscription_pay sbp
INNER JOIN
    court_loan.member_subscriptions sb
ON
    (
        sbp.member_subscriptions_id = sb.id)
INNER JOIN
    court_loan.member m
ON
    (
        sb.member_id = m.id)
WHERE m.member_id=$P{member_code} ;]]>
	</queryString>
