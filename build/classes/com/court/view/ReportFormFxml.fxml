<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.court.controller.ReportFormFxmlController">
    <children>
        <FlowPane alignment="CENTER_LEFT" hgap="10.0" vgap="10.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="mbr_loans_btn" mnemonicParsing="false" onAction="#onLoanAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="All Loans">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-success" />
                    </styleClass>
                </Button>
                <Button mnemonicParsing="false" onAction="#onMemberAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="All Members By Working Office">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-warning" />
                    </styleClass> 
                </Button>
            <Button mnemonicParsing="false" onAction="#onMemberPayOfficeAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="All Members By Payment Office">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-primary" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#onBranchAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="All Branches">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-success" />
               </styleClass>
            </Button>
                <Button mnemonicParsing="false" onAction="#onMemberWiseLoanAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="Member Wise Loans">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-primary" />
                    </styleClass> 
                </Button>
                <Button mnemonicParsing="false" onAction="#onBranchCollectionDueAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="Branch Wise Collection Due">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-success" />
                    </styleClass>
                </Button>
                <Button mnemonicParsing="false" onAction="#onBranchCollectionMadeAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="Branch Wise Collection Made">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-danger" />
                    </styleClass>
                </Button>
                <Button mnemonicParsing="false" onAction="#onMemberHistoryAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="Member History">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-warning" />
                    </styleClass>
                </Button>
            <Button mnemonicParsing="false" onAction="#onMemberSubscriptionAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="Member Wise Subscription">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-danger" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#onReceiptPaymentMadeAction" prefHeight="54.0" style="-fx-text-fill: #ffff;" text="Receipt Payment Made">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-primary" />
               </styleClass>
            </Button>
            </children>
        </FlowPane>
    </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
